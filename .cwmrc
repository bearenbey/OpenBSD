sticky yes
snapdist 4
gap 0 0 0 0
fontname "spleen:pixelsize=32"
unbind-key all

borderwidth 4
color activeborder "#55aaaa"
color inactiveborder "#9eefef"
color menubg "#ffffff"
color menufg "#00000"

bind-key M-Return terminal
bind-key 4-l lock
bind-key M-BackSpace window-hide

bind-key M-Down window-lower
bind-key M-Up window-raise
bind-key M-Tab window-cycle
bind-key MS-Tab window-rcycle
bind-key M-x window-delete

bind-key M-1 group-only-1
bind-key M-2 group-only-2
bind-key M-3 group-only-3
bind-key M-4 group-only-4
bind-key M-5 group-only-5
bind-key M-6 group-only-6
bind-key M-7 group-only-7

bind-key MS-1 window-movetogroup-1
bind-key MS-2 window-movetogroup-2
bind-key MS-3 window-movetogroup-3
bind-key MS-4 window-movetogroup-4
bind-key MS-5 window-movetogroup-5
bind-key MS-6 window-movetogroup-6
bind-key MS-7 window-movetogroup-7

bind-key M-a group-toggle-all
bind-key M-g window-group

bind-key M-Right group-cycle
bind-key M-Left group-rcycle
bind-key M-s window-stick

bind-key M-f window-maximize
bind-key M-equal window-vmaximize
bind-key MS-equal window-hmaximize

bind-key M-h window-move-left-big
bind-key M-j window-move-right-big
bind-key M-k window-move-down-big
bind-key M-l window-move-up-big
bind-key M-n window-resize-up-big
bind-key M-m window-resize-down-big
bind-key M-v window-resize-left-big
bind-key M-b window-resize-right-big

bind-key M-question menu-cmd
bind-key M-p menu-exec

bind-key M-q restart
bind-key MS-q quit

ignore xclock
